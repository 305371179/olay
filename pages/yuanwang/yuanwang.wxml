<view class="stage yuanwang" name="yuanwang" id="yuanwang">
    <swiper class="{{'page'+current}}" style="{{'background:url('+baseUrl +'shouye/bg.png)'}}"
            indicator-dots="{{false}}" current="{{current}}" bindchange="bindchange"
            autoplay="{{false}}" vertical="{{false}}">
        <!--家庭-->
        <swiper-item>
            <image class="bitmap9 _id16" name="contentBg" src="{{baseUrl+'yuanwang/contentBg.png'}}"></image>
            <image class="bitmap11 _id14" name="img0" src="{{baseUrl+'yuanwang/img0.png'}}"></image>
            <image class="bitmap10 _id15" name="title0" src="{{baseUrl+'yuanwang/title0.png'}}"></image>
            <image class="bitmap8 _id17" name="contentText0" src="{{baseUrl+'yuanwang/contentText0.png'}}"></image>
            <image bindtap="bindtap" class="bitmap5 _id20" name="upload"
                   src="{{baseUrl+'yuanwang/upload.png'}}"></image>
        </swiper-item>
        <!--爱情-->
        <swiper-item>
            <image class="bitmap9 _id16" name="contentBg" src="{{baseUrl+'yuanwang/contentBg.png'}}"></image>
           
            <image class="bitmap11 _id14" name="img1" src="{{baseUrl+'yuanwang/img1.png'}}"></image>
            <image class="bitmap10 _id15" name="title1" src="{{baseUrl+'yuanwang/title1.png'}}"></image>
            <image class="bitmap12 _id13" name="contentText1" src="{{baseUrl+'yuanwang/contentText1.png'}}"></image>


            <image bindtap="bindtap" class="bitmap5 _id20" name="upload"
                   src="{{baseUrl+'yuanwang/upload.png'}}"></image>
        </swiper-item>
        <!--梦想-->
        <swiper-item>
            <image class="bitmap9 _id16" name="contentBg" src="{{baseUrl+'yuanwang/contentBg.png'}}"></image>

            <image class="bitmap16 _id9" name="contentText2" src="{{baseUrl+'yuanwang/contentText2.png'}}"></image>
            <image class="bitmap15 _id10" name="img2" src="{{baseUrl+'yuanwang/img2.png'}}"></image>
            <image class="bitmap14 _id11" name="title2" src="{{baseUrl+'yuanwang/title2.png'}}"></image>


            <image bindtap="bindtap" class="bitmap5 _id20" name="upload"
                   src="{{baseUrl+'yuanwang/upload.png'}}"></image>
        </swiper-item>
        <!--事业-->
        <swiper-item>
            <image class="bitmap9 _id16" name="contentBg" src="{{baseUrl+'yuanwang/contentBg3.png'}}"></image>
            <image class="bitmap19 _id6" name="contentText3" src="{{baseUrl+'yuanwang/contentText3.png'}}"></image>
